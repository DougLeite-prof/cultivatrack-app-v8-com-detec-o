# Modelos de Machine Learning
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.onnx filter=lfs diff=lfs merge=lfs -text

# Imagens
*.jpg binary
*.jpeg binary  
*.png binary
*.gif binary
*.bmp binary
*.tiff binary

# Documentos
*.pdf binary

# Configuração de line endings
*.py text eol=lf
*.md text eol=lf
*.txt text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.json text eol=lf
*.js text eol=lf
*.css text eol=lf
*.html text eol=lf

# Dockerfile
Dockerfile text eol=lf
